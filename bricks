def load_gazebo_models(table_pose=Pose(position=Point(x=1.0, y=0.0, z=-0.3)),
                       table_reference_frame="world",
                       brick1_pose=Pose(position=Point(x=0.6725, y=0.126, z=0.5)), # brick 1
                       brick1_reference_frame="world",
                       brick2_pose=Pose(position=Point(x=0.7725, y=0.126, z=0.5)), # brick 2
                       brick2_reference_frame="world",
                       brick3_pose=Pose(position=Point(x=0.8725, y=0.126, z=0.5)), # brick 3
                       brick3_reference_frame="world",
                       brick4_pose=Pose(position=Point(x=0.9725, y=0.126, z=0.5)), # brick 4
                       brick4_reference_frame="world",
                       brick5_pose=Pose(position=Point(x=0.10725, y=0.126, z=0.5)), # brick 5
                       brick5_reference_frame="world",
                       brick6_pose=Pose(position=Point(x=0.5725, y=0.126, z=0.5)), # brick 6
                       brick6_reference_frame="world",
                       brick7_pose=Pose(position=Point(x=0.4725, y=0.126, z=0.5)), # brick 7
                       brick7_reference_frame="world",
                       brick8_pose=Pose(position=Point(x=0.3725, y=0.126, z=0.5)), # brick 8
                       brick8_reference_frame="world",
                       brick9_pose=Pose(position=Point(x=0.2725, y=0.126, z=0.5)), # brick 9
                       brick9_reference_frame="world",
                       brick10_pose=Pose(position=Point(x=0.1725, y=0.126, z=0.5)), # brick 10
                       brick10_reference_frame="world",
                       brick11_pose=Pose(position=Point(x=0.11725, y=0.126, z=0.5)), # brick 11
                       brick11_reference_frame="world",
                       brick12_pose=Pose(position=Point(x=0.12725, y=0.126, z=0.5)), # brick 12
                       brick12_reference_frame="world",
                       brick13_pose=Pose(position=Point(x=0.13725, y=0.126, z=0.5)), # brick 13
                       brick13_reference_frame="world",
                       brick14_pose=Pose(position=Point(x=0.14725, y=0.126, z=0.5)), # brick 14
                       brick14_reference_frame="world",
                       brick15_pose=Pose(position=Point(x=0.15725, y=0.126, z=0.5)), # brick 15
                       brick15_reference_frame="world",
                       brick16_pose=Pose(position=Point(x=0.6725, y=0.226, z=0.5)), # brick 16
                       brick16_reference_frame="world",
                       brick17_pose=Pose(position=Point(x=0.6725, y=0.326, z=0.5)), # brick 17
                       brick17_reference_frame="world",
                       brick18_pose=Pose(position=Point(x=0.6725, y=0.426, z=0.5)), # brick 18
                       brick18_reference_frame="world",
                       brick19_pose=Pose(position=Point(x=0.6725, y=0.526, z=0.5)), # brick 19
                       brick19_reference_frame="world",
                       brick20_pose=Pose(position=Point(x=0.6725, y=0.626, z=0.5)), # brick 20
                       brick20_reference_frame="world",
                       brick21_pose=Pose(position=Point(x=0.6725, y=0.726, z=0.5)), # brick 21
                       brick21_reference_frame="world",
                       brick22_pose=Pose(position=Point(x=0.6725, y=0.826, z=0.5)), # brick 22
                       brick22_reference_frame="world",
                       brick23_pose=Pose(position=Point(x=0.6725, y=0.926, z=0.5)), # brick 23
                       brick23_reference_frame="world",
                       brick24_pose=Pose(position=Point(x=0.6725, y=0.1026, z=0.5)), # brick 24
                       brick24_reference_frame="world",
                       brick25_pose=Pose(position=Point(x=0.6725, y=0.1126, z=0.5)), # brick 25
                       brick25_reference_frame="world",
                       brick26_pose=Pose(position=Point(x=0.6725, y=0.1226, z=0.5)), # brick 26
                       brick26_reference_frame="world"):

    # Get Models' Path
    model_path = rospkg.RosPack().get_path('baxter_sim_examples')+"/models/"
    # Load Table SDF
    table_xml = ''
    with open (model_path + "cafe_table/model.sdf", "r") as table_file:
        table_xml=table_file.read().replace('\n', '')
    # Load Brick1 SDF
    brick1_xml = ''
    with open (model_path + "new_brick/model.sdf", "r") as brick1_file:
        brick1_xml=brick1_file.read().replace('\n', '')
    # Load Brick2 SDF
    brick2_xml = ''
    with open (model_path + "new_brick/model.sdf", "r") as brick2_file:
        brick2_xml=brick2_file.read().replace('\n', '')
    # Load Brick3 SDF
    brick3_xml = ''
